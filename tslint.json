{
    "extends": "tslint:recommended",
    "rules": {
        "object-literal-sort-keys": false,
        "new-parens": true,
        "no-arg": true,
        "no-conditional-assignment": true,
        "no-console": {
            "severity": "warning",
            "options": [
                "debug",
                "info",
                "log",
                "time",
                "timeEnd",
                "trace"
            ]
        },
        "quotemark": [
            true,
            "single"
        ]
    },
    "jsRules": true
}
